<%= content_for :content do %>
	<div class="detail">
		<div class="detail-header">
			<div class="row row-eq-height">
				<div class="col-lg-3 col-sm-3">
					<div class="main-img"><%= detail_header_thumb %></div>
				</div>
				<div class="col-lg-5 col-sm-5">
					<div class="primary-info">
						<%= detail_header_primary_info %>
					</div>
				</div>
				<div class="col-lg-4 col-sm-4 col-valign-bottom">
					<div class="secondary-info">
						<%= detail_header_secondary_info %>
					</div>
				</div>
			</div>
		</div>
		<div class="detail-nav main-section-nav">
			<div class="row">
				<div class="col-lg-8">
					<div class="detail-nav-left">
						<%= detail_header_nav_left %>
					</div>
				</div>
				<div class="col-lg-4">
					<div class="detail-nav-right">
						<%= detail_header_nav_right %>
					</div>
				</div>
			</div>
		</div>
		<div class="row">
			<div class="col-lg-8">
				<%= yield %>
			</div>
			<div class="col-lg-4">
				<%= detail_right_col %>
			</div>
		</div>
	</div>
<% end %>

<%= render template: "layouts/application" %>